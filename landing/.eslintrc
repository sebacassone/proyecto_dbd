{
  "env": {
    "browser": true,
    "node": true,
    "amd": true
  },
  "extends": "react-app",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/forbid-prop-types": 0,
    "react/prop-types": 0,
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}